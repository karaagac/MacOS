pandoc filename.md -s -o filename.pdf;;convert markdown to pdf
espanso start;;textexpander
http://localhost:8384/ ;;syncthing
https://linuxhint.com/install_oracle_jdk11_arch_linux/ ;;java 11 install
http://localhost:8080/__admin;;WireMock server
http://localhost:8080/__admin/recorder/;;WireMock Server Interface
:%!jq .;;json file format vim api
pandoc example.md -o example.pdf --from markdown --template eisvogel --listings;;markdown to pdf export with table of content
pandoc Selenium.md -o example.pdf --from markdown --template eisvogel --listings --toc --pdf-engine=xelatex;; markdown to pdf export table of contents
control + r ;;search history fzf keybindings shell
control + t ;;search current folder keybindings shell
c-f ;;search files in current dir ranger keybindings 
c-g ;;search files globally ranger keybindings 
c-f ;;search files in vim keybindings 
c-f ;;search buffers in vim keybindings 
c-t ;;search any text in vim keybindings 

# API===
100;;Countinue httpStatusCode
102;;Processing httpStatusCode
200;;OK httpStatusCode
201;;Created httpStatusCode
304;;Not Modified httpStatusCode
400;;Bad Request httpStatusCode
401;;Unauthorized httpStatusCode
403;;Forbidden httpStatusCode
404;;Not Found httpStatusCode
504;;Gataway Error - Server Side Error (Cant connect to server) httpStatusCode

# SELENIUM LOCATORS
tagname[@attribute='value'] ;; looks for tagname attribute and value xpath locators
tagname[contains(@attribute, 'value')] ;; if a locator has some static part, dynamic, contains, xpath, locators
tagname[.='text'] ;; looks for exact text match for any attribute xpath, locators
//label[startswith(@id, 'message')];;xpath, locators, dynamic
//td[text()='usedId'];;xpath, locators, dynamic
//input[@type='submit' AND @name='login'];;xpath, locators
//input[@type='submit' OR @name='login'];;xpath, locators
//input[@id='value' and @type='value'];;multiple xpath locators with and
//a[text()='textvalue'];;xpath link text dynamic
//a[contains(text(), 'Forgot your')];;xpath link text contains  dynamic https://ui.cogmento.com/
//a[text()='text'];;xpath, dynamic
//span[text()='text'];;find text dynamic
//a[@text()='text2 test2']//parent::td[@class='detailstrow'];;parent xpath locators
//a[@text()='text2 test2']//parent::td[@class='detailstrow']//preceding-sibling::td[@class='detailstrow']//input[@name='contact_id'];;preceding backward previous sibling  xpath locators
following-sibling;;xpath locators
preceding-sibling;;xpath locators
img[src$='.jpg'];;locator image css

WebDriverWait wait = new WebDriverWait(driver,30);; explicitWait selenium


htmltag#id;;css selector id
htmltag#classname;;css selector classname
htmltag.classVALUE;;css selector classname use classvalue only
htmltag#idVALUE.classVALUE;;css selector both id and classname combined
htmltag.partialclassnameVALUE;;css selector partial class names value
.classVALUE;;css selector classvalue only
htmltag[id='value'];;css selector id
htmltag[id='value'][type='value'];;css selector multiple values
input[id*='user'];;css selector. You can also use: contains input[id*='name'] for username
input[id^='user'];;css selector start with
input[id$='name'];;css selector ending with
ul#categories>li:first-of-type;;css selector taken from: https://guide.blazemeter.com/hc/en-us/articles/360002052998-The-Continuous-Testing-Journey
ul#categories>li:last-of-type;;css selector taken from: https://guide.blazemeter.com/hc/en-us/articles/360002052998-The-Continuous-Testing-Journey
ul#categories>li:nth-of-type(5);;css selector 5th elementtaken from: https://guide.blazemeter.com/hc/en-us/articles/360002052998-The-Continuous-Testing-Journey
ul#categories>li:nth-of-type(n);;css selector all elements taken from: https://guide.blazemeter.com/hc/en-us/articles/360002052998-The-Continuous-Testing-Journey

parenttag>childtag;;css selector

contextClick().perform() ;;actions right click mouse selenium
dragAndDrop().perform ;;actions drag and drop mouse selenium

